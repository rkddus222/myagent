{
  "version": 2,
  "buildCommand": "cd front && npm install && npm run build",
  "outputDirectory": "front/dist",
  "installCommand": "cd front && npm install",
  "devCommand": "cd front && npm run dev",
  "framework": "vite",
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 